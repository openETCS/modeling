						<Transition kind="restart">
							<target>
								<StateRef name="SH_initiated_by_driver_procedure_off"/>
							</target>
							<condition>
								<!--  not Loc_Procedure_Possible or  not Train_Standstill -->
								<NAryOp name="2" operator="or">
									<operands>
										<!--  not Loc_Procedure_Possible -->
